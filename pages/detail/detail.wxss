.container {
    height: 100%;
    width: 100%;
    box-sizing: border-box;
    background: #f8f8f8;
} 
swiper{
    width: 100%;
    height: 750rpx;
}
swiper-item image{
    width: 100%;
    height: 100%;
}
.proMsg{
    padding-top: 20rpx;
	  background-color: white;
    width: 100%;
    margin-bottom: 20rpx;
    position: relative;
}
.proMsg .proName{
	padding: 10rpx 30rpx;
	font-size: 32rpx;
	line-height: 30rpx; 
	color: #333;
}
.proMsg .price{
	color: #ff6662;
	font-size: 36rpx;
	margin-left: 30rpx;
	line-height: 80rpx;
}
.proMsg .salse{
	display: flex;
	padding: 10rpx 20rpx;
	text-align: center;
	border: 1rpx solid #f3f3f3;
	font-size: 28rpx;
	color: #888888;
	line-height: 60rpx;
}
.proMsg .salse view{
	width: 33.33%;
}
.proMsg .salse .middle{
	border-left: 1rpx solid #f3f3f3;
	border-right: 1rpx solid #f3f3f3;
}

.open-device{
    height: 780rpx;
    width: 100%;
    position: fixed; 
    bottom:-1000rpx;
    left: 0%;
    margin-left: 0px;
    background-color: #fff;
    transition: all 0.3s ease-in-out;  
    z-index: 999;
    border-radius: 0px;
}
.mask{
    width: 100%;
    height: 100%;
    background: #000;
    opacity: 0.6;
    position: fixed;
    left: 0rpx;
    top: 0rpx;
    z-index: 99;
}
.info{
    display: flex;
    flex-direction: row;
    padding: 20rpx 30rpx;
    padding-bottom: 0rpx;
    position: relative;
}
.info:after{
    content: "";
    left: 0rpx;
    bottom: 20rpx;
    height: 2rpx;
    width: 100%;
    background: #e9e9e9;
    position: absolute;
}
.info .img{
    width: 200rpx;
    height: 200rpx;
    position:relative;
    top: -50rpx;
    border: 2rpx solid #e9e9e9;
    background: #fff;
    border-radius: 4rpx;
    box-sizing: border-box;
}
.info .img image{
    width: 196rpx;
    height: 196rpx;
    border-radius: 4rpx;
}
.info .desc{
    flex: 1;
    box-sizing: border-box;
    padding-left: 30rpx;
    font-size: 30rpx;
}
.info .title{
    color: #333;
    height: 50rpx;
    line-height: 50rpx;
}
.info .price{
    height: 50rpx;
    line-height: 50rpx;
    color: #666;
}
.info .price text{
    color: #FF8247;
}
.info .rest{
    font-size: 30rpx;
    color: #666;
    height: 50rpx;
    line-height: 50rpx;
}
.size{
    border-bottom: 2rpx solid #e9e9e9;
    padding: 10rpx 30rpx;
    padding-bottom: 0rpx;
}
.size .title{
    color: #333;
    height: 50rpx;
    line-height: 50rpx;
    font-size: 28rpx;
}
.size .detail{
    color: #333;
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}
.size .detail .item{
    margin-bottom: 24rpx;
    box-sizing: border-box;
    padding: 12rpx 6rpx;
    text-align: center;
    background: #fff;
    border-radius: 4rpx;
    font-size: 24rpx;
    width:calc( 25% - 10rpx );
    border: 2rpx solid #dddddd;
    color: #777777;
}
.size .detail .item:nth-child(4n+1){
    margin-right: 15rpx;
}
.size .detail .item:nth-child(4n+2){
    margin-right: 15rpx;
}
.size .detail .item:nth-child(4n+3){
    margin-right: 10rpx;
}
.size .detail .item:nth-child(4n+0){
    margin-right: 0rpx;
}
.size .detail .item.active{
    background: #F3F6FF;
    border: 2rpx solid #22A3FE;
    color: #22A3FE;
    position: relative;
}
.size .detail .item.active:before{
    content: '';
    position: absolute;
    right:0;
    bottom:0;
    width: 0;
    height:0;
    border-right: 22rpx solid #22A3FE;
    border-left: 22rpx solid transparent;
    border-top: 18rpx solid transparent;
    border-bottom: 18rpx solid #22A3FE;
}
.size .detail .item.active:after{
    content: '';
    position: absolute;
    right:8rpx;
    bottom:6rpx;
    width: 8rpx;
    height: 16rpx;
    border-right: 2rpx solid white;
    border-bottom: 2rpx solid white;
    transform: rotate(45deg);
}
.count{
    border-bottom: 2rpx solid #e9e9e9;
    padding: 20rpx 30rpx;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
}
.count .title{
    color: #333;
    font-size: 28rpx;
    height: 60rpx;
    line-height: 60rpx;
}
.num{
    display: flex;
    margin-top: 6rpx;
    height: 54rpx;
    width: 200rpx;
    text-align: center;
    line-height: 54rpx;
    border: 1rpx solid #e9e9e9;
    font-size: 28rpx;
}
.num .nownum{
    width: 100rpx;
    border-left: 1rpx solid #e9e9e9;
    border-right: 1rpx solid #e9e9e9;
    font-size: 24rpx;
}
.disnum, .addnum{
    width: 54rpx;
    display: flex;
    align-items: center;
    justify-content: center;
}


.goods-info{
    background-color: #fff;
    border-bottom-left-radius: 10rpx;
    border-bottom-right-radius: 10rpx;
}
.ginfo{
    color: #555;
    padding: 20rpx 30rpx;
    line-height: 28rpx;
    font-size: 24rpx;
}
.ginfo .item{
    border-bottom: 2rpx solid #f8f8f8;
    height: 70rpx;
    line-height: 70rpx;
}
.ginfo .item:last-child{
    border: none;
}
.ginfo .item.select{
    position: relative;
}
.ginfo .item.select:after{
    content: '';
    position: absolute;
    right:30rpx;
    top:50%;
    width: 16rpx;
    height: 16rpx;
    border-right: 2rpx solid #d2d2d2;
    border-bottom: 2rpx solid #d2d2d2;
    transform: rotate(-45deg) translateY(-50%);
}
.ginfo text{
    color: #777;
}
.ginfo .other{
    margin-top: 30rpx;
    text-align: center;
}
.ginfo .other text{
    padding: 0 10rpx;
}

.details{
    padding: 30rpx 0;
    padding-bottom: 100rpx;
    font-size:28rpx;
}
.details .title{
    text-align: center;
    position: relative;
    margin-bottom: 30rpx;
    margin-top: 20rpx;
    height: 50rpx;
    line-height: 50rpx;
}
.details .title .about{
    position: absolute;
    left: 50%;
    top: 50%;
    transform: translate(-50%,-50%);
    width: 90px;
    z-index: 99;
    background: #F7F7F7;
}
.details .title:after{
    content: '';
    border-bottom: 2rpx solid #d2d2d2;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translateX(-50%);
    width: 80%;
}
.details .desc{
    min-height: 300rpx;
}
.details button{
    width: 600rpx;
    height: 108rpx;
    color: #fff;
    font-size: 36rpx;
    font-weight: 400;
    line-height: 108rpx;
    text-align: center;
    background-image: linear-gradient(90deg, #7C92FF 0%, #22A3FE 100%);
    border-radius: 200rpx;
    margin-top: 60rpx;
}
.detail-image{
    width: 100%;
    display: block;
    margin: 0;
}

.operate{
    width: 100%;
    height: 128rpx;
    line-height: 128rpx;
    text-align: center;
    color: white;
    font-size: 36rpx;
    position: absolute;
    left: 0rpx;
    bottom: 0rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    background: white;
    margin-top: 20rpx;
}
.operate .btn{
    font-size: 32rpx;
    background-image: linear-gradient(90deg, #7C92FF 0%, #22A3FE 100%);
    border-radius: 5px;
    width: calc( 100% - 60rpx );
    height: 88rpx;
    line-height: 88rpx;
}

.dbline{
    line-height: 40rpx;
    padding-top: 10rpx;
    padding-bottom: 10rpx;
    display: flex;
    flex-direction: column;
}
.gg{
    position: relative;
}
.pt-alert{
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 90rpx;
    line-height: 90rpx;
    color: white;
    display: flex;
    flex-direction: row;
    background: rgba(255,0,0, 0.7);
    z-index: 9999;
}
.pt-alert .left{
    width: calc( 100% - 400rpx );
    height: 100%;
    color: white;
    font-size: 48rpx;
    padding-left: 30rpx;
    box-sizing: border-box;
}
.pt-alert .right{
    width: 400rpx;
    height: 100%;
    color: white;
    font-size: 28rpx;
    text-align: right;
    padding-right: 30rpx;
    box-sizing: border-box;
}