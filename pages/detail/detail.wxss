.footfixed { 
    display: flex; flex-direction: row; position: fixed; bottom: 0; left: 0;
    background: #fff; z-index: 100; width:100%; font-size: 14px;
    line-height: 100rpx;
}
.footfixed:after{
  content: '';
  position:absolute;
  right:0;
  top:0;
  height:2rpx;
  bottom:0;
  width:100%;
  transform: scaleY(0.5);
  background: #e9e9e9;
}
.footfixed .bt{
   display: flex; flex-direction: row;
}
