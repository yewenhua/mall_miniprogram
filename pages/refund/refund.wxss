page{
  height: 100%;
  font-family: PingFangSC-Regular;
}
.star-page{
  height: 100%;
  overflow: auto;
  box-sizing: border-box;
  background: #f7f7f7;
}
.star-title{
    color: #9F9C9C;
    font-size: 24rpx;
    margin-top: 30rpx;
    padding-left: 30rpx;
    height: 50rpx;
    line-height: 50rpx;
}
.bb{
    border-bottom: 2rpx solid #e8eaec;
}
.o-btn{
    padding: 20rpx 30rpx;
}
.o-btn button{
    width: 100%;
    height: 88rpx;
    color: #fff;
    font-size: 32rpx;
    font-weight: 400;
    line-height: 88rpx;
    text-align: center;
    background-image: linear-gradient(90deg, #7C92FF 0%, #22A3FE 100%);
    border-radius: 10rpx;
    margin-top: 30rpx;
    border: none;
}
.o-btn .sb-hover{
    background: #22A3FE;
}
.o-text{
    height: 200rpx;
    width: 100%;
    border: 2rpx solid #e8eaec;
    padding: 16rpx;
    box-sizing: border-box;
    border-bottom: none;
    font-size: 28rpx;
}
.o-content{
    background: white;
}
.o-content .title{
    font-size: 30rpx;
    margin-top: 30rpx;
    margin-bottom: 10rpx;
    color:#999;
}
.up-img{
    padding: 20rpx 30rpx;
    display: flex;
    flex-direction: row;
}
.up-img-item{
    width: 120rpx;
    box-sizing: border-box;
    margin-right: 22rpx;
    position: relative;
}
.up-img-item.more:last-child{
    margin-left: 0rpx;
    margin-right: 0rpx;
}
.up-img-item image{
    width: 120rpx;
    height: 120rpx;
    display: block;
}
